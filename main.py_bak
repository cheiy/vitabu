from flask import Flask, redirect, url_for, render_template

app = Flask(__name__)

@app.route("/")
def main():
    return render_template("index.html")

@app.route("/<name>")
def user():
    return f"Hello, {name}!"

@app.route("/admin")
def admin():
    #return redirect(url_for("user", name="Admin!"))
    return f"Hello, Admin"
if __name__ == "__main__":
    app.run(debug=True)

